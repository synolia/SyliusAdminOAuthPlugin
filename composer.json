{
    "name": "synolia/sylius-admin-oauth-plugin",
    "type": "sylius-plugin",
    "description": "Oauth plugin for Sylius admin.",
    "keywords": [
        "sylius",
        "sylius-plugin"
    ],
    "license": "EUPL-1.2",
    "require": {
        "php": "^7.4 || ^8.0",
        "league/oauth2-client": "^2.7",
        "league/oauth2-google": "^4.0",
        "monsieurbiz/sylius-settings-plugin": "^1.0",
        "sylius/sylius": "^1.9"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.11.0",
        "friendsoftwig/twigcs": "6.0.0",
        "j13k/yaml-lint": "1.1.4",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "phpmd/phpmd": "2.13.0",
        "phpro/grumphp": "1.13.0",
        "phpstan/extension-installer": "1.1.0",
        "phpstan/phpstan": "1.8.6",
        "phpstan/phpstan-doctrine": "1.3.14",
        "phpstan/phpstan-phpunit": "1.1.1",
        "phpstan/phpstan-strict-rules": "1.4.4",
        "phpstan/phpstan-webmozart-assert": "1.2.1",
        "phpunit/phpunit": "9.5.24",
        "povils/phpmnd": "3.0.1",
        "sebastian/phpcpd": "6.0.3",
        "seld/jsonlint": "1.9.0",
        "squizlabs/php_codesniffer": "3.7.1",
        "sylius-labs/coding-standard": "4.1.1",
        "symfony/browser-kit": "5.4.11",
        "symfony/debug-bundle": "5.4.11",
        "symfony/dotenv": "5.4.5",
        "symfony/web-server-bundle": "4.4.44",
        "webmozart/assert": "1.11.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true
        }
    },
    "autoload": {
        "psr-4": {
            "Synolia\\SyliusAdminOauthPlugin\\": "src/",
            "Synolia\\Tests\\SyliusAdminOauthPlugin\\": "tests/"
        }
    },
    "prefer-stable": true
}
