{
    "name": "synolia/sylius-admin-oauth-plugin",
    "type": "sylius-plugin",
    "description": "Oauth plugin for Sylius admin.",
    "keywords": [
        "sylius",
        "sylius-plugin"
    ],
    "license": "EUPL-1.2",
    "require": {
        "php": "^8.0",
        "knpuniversity/oauth2-client-bundle": "^2.15",
        "league/oauth2-google": "^4.0",
        "monsieurbiz/sylius-settings-plugin": "^1.0",
        "sylius/sylius": "^1.10"
    },
    "require-dev": {
        "friendsofphp/php-cs-fixer": "3.16.0",
        "friendsoftwig/twigcs": "6.2.0",
        "j13k/yaml-lint": "1.1.5",
        "php-parallel-lint/php-parallel-lint": "1.3.2",
        "phpmd/phpmd": "2.13.0",
        "phpro/grumphp": "1.16.0",
        "phpstan/extension-installer": "1.3.0",
        "phpstan/phpstan": "1.10.15",
        "phpstan/phpstan-doctrine": "1.3.40",
        "phpstan/phpstan-phpunit": "1.3.11",
        "phpstan/phpstan-strict-rules": "1.5.1",
        "phpstan/phpstan-webmozart-assert": "1.2.4",
        "phpunit/phpunit": "9.6.8",
        "povils/phpmnd": "3.1.0",
        "rector/rector": "^0.16.0",
        "seld/jsonlint": "1.10.0",
        "squizlabs/php_codesniffer": "3.7.2",
        "sylius-labs/coding-standard": "4.3.0",
        "symfony/browser-kit": "6.0.19",
        "symfony/debug-bundle": "6.0.19",
        "symfony/dotenv": "6.0.19",
        "symfony/web-server-bundle": "4.4.44",
        "webmozart/assert": "1.11.0"
    },
    "config": {
        "sort-packages": true,
        "allow-plugins": {
            "dealerdirect/phpcodesniffer-composer-installer": true,
            "symfony/thanks": true,
            "phpro/grumphp": true,
            "phpstan/extension-installer": true,
            "php-http/discovery": true
        }
    },
    "autoload": {
        "psr-4": {
            "Synolia\\SyliusAdminOauthPlugin\\": "src/",
            "Synolia\\Tests\\SyliusAdminOauthPlugin\\": "tests/"
        }
    },
    "prefer-stable": true
}
